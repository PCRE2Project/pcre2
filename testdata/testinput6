    axxxyzq
    axxxxyzq
\= Expect no match
    ax
    axx     
    ayzq
    axyzq
    axxyz
      
/x{2,3}yz/
    axxyz
    axxxyzq
    axxxxyzq
\= Expect no match
    ax
    axx     
    ayzq
    axyzq
      
/[^a]+/no_auto_possess
    bac
    bcdefax
\= Expect no match
    aaaaa   

