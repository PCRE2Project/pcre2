    \x{b4}

/[[:^ascii:]]/utf,ucp
    \x{100}
    \x{200}
    \x{300}
    \x{37e}
\= Expect no match
    aa
    99

/[[:^ascii:]\w]/utf,ucp
    aa
    99
    gg
    \x{100}
    \x{200}
    \x{300}
    \x{37e}

/[\w[:^ascii:]]/utf,ucp
    aa
    99
    gg
    \x{100}
    \x{200}
    \x{300}
    \x{37e}

/[^[:ascii:]\W]/utf,ucp
    \x{100}
    \x{200}
\= Expect no match
    aa
    99
    gg
    \x{37e}

/[^[:^ascii:]\d]/utf,ucp
    a
    ~
    \a
    \x{7f}
\= Expect no match
    0
    \x{389}
    \x{20ac}

/(?=.*b)\pL/
    11bb

/(?(?=.*b)(?=.*b)\pL|.*c)/
    11bb

