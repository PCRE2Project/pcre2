     acfX1zzzaefX2zzzadeefX3zzzX4zzz

/(?<!a(b?c|d?e?e)f)X./g
     acfX1zzzaefX2zzzadeefX3zzzX4zzz
     
/(?(?<=ab?c)d|e)/
    abcdefg
    acdefg
    axdefg
    
